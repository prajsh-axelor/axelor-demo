<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
	
	<grid name="order-line-grid" title="Ordered Items" model="com.axelor.demoimpl.db.OrderLine">
		<field name="order" />
		<field name="product" />
		<field name="price" />
		<field name="quantity" />
	</grid>
	
	<form name="order-line-form" title="Ordered Item" model="com.axelor.demoimpl.db.OrderLine">
		<panel title="Overview">
			<field name="product" colSpan="12"/>
			<field name="price" />
			<field name="price" />
			<field name="quantity" />
		</panel>
	</form>
</object-views>